{
    "version": 2,
    "builds": [
      { "src": "backend/api/server.js", "use": "@vercel/node" },
      { "src": "frontend/build/**", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "backend/api/server.js" },
      { "src": "/(.*)", "dest": "frontend/build/$1" }
    ]
  }